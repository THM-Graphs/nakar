{
  "name": "nakar-server",
  "version": "1.0.0",
  "private": true,
  "description": "A Strapi application",
  "scripts": {
    "build": "npm run build:gen && strapi build",
    "build:gen": "openapi-typescript ./openapi.yaml -o ./src-gen/schema.d.ts --root-types",
    "develop": "strapi develop",
    "start": "strapi start",
    "lint": "eslint src",
    "lint:fix": "eslint --fix .",
    "audit": "npm audit --audit-level none",
    "test": "jest",
    "export": "strapi export --no-compress --no-encrypt -f export",
    "import": "strapi import -f export.tar",
    "transfer": "strapi transfer --from http://ip2.mni.thm.de:1337/admin",
    "version": "echo $npm_package_version"
  },
  "dependencies": {
    "@strapi/plugin-documentation": "^5.6.0",
    "@strapi/plugin-users-permissions": "5.7.0",
    "@strapi/strapi": "5.7.0",
    "better-sqlite3": "11.3.0",
    "handlebars": "^4.7.8",
    "http-errors": "^2.0.0",
    "mysql2": "^3.12.0",
    "neo4j-driver": "^5.27.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0",
    "ts-pattern": "^5.6.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.17.0",
    "@jest/globals": "^29.7.0",
    "@types/http-errors": "^2.0.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.7.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "openapi-typescript": "^7.4.4",
    "prettier": "3.4.2",
    "supertest": "^7.0.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "uuid": "4257914e-0ce5-42cf-91c6-ffa7a5ecd95a"
  }
}
